{
  "name": "read-remove-file",
  "version": "1.0.0",
  "description": "Read a file, then remove it",
  "repository": "shinnn/node-read-remove-file",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint *.js && jscs *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/node-read-remove-file/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "file",
    "fs",
    "read",
    "remove",
    "rm",
    "tmp",
    "temporary",
    "disposable",
    "async",
    "asynchronous"
  ],
  "dependencies": {
    "graceful-fs": "^3.0.5",
    "is-relative": "^0.1.3",
    "rimraf": "^2.2.8"
  },
  "devDependencies": {
    "del": "^1.1.0",
    "eslint": "^0.10.2",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "output-file-sync": "^1.1.0",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
